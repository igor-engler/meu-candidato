{
    "openapi": "3.0.0",
    "info": {
        "title": "Meu-Candidato Documentation",
        "description": "This is an API",
        "version": "1.0.0",
        "contact": {
            "email": "marcosmuce@gmail.com"
        }
    },
    "paths": {
        "/users": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Cria um usuário",
                "description": "Cria um novo usuário",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "email": "teste@teste.com.br",
                                    "name": "Fulano Da Silva Sauro",
                                    "password": "12345678"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Usuário criado com sucesso"
                    },
                    "500": {
                        "description": "Usuário já existe"
                    }
                }
            },
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Lista todos os usuários.",
                "description": "Lista todos os usuário cadastrados no sistema.",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "itens": {
                                        "type": "object",
                                        "properties": {
                                            "email": {
                                                "type": "string"
                                            },
                                            "name": {
                                                "type": "string"
                                            },
                                            "password": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}